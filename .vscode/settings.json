{
  "cSpell.words": ["Astro"],
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "emmet.includeLanguages": {
    "astro": "html"
  },
  "files.exclude": {
    "**/dist": true,
    "**/node_modules": true
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "search.exclude": {
    "**/dist": true
  },
  "typescript.inlayHints.parameterNames.enabled": "all"
}
